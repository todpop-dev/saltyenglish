            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:src="@drawable/store_33_image_peoplebar" />

        <ImageView
            android:id="@+id/home_my_page_attendance_id_textballoon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@id/home_my_page_attendance_id_peoplebar"
            android:layout_alignLeft="@id/home_my_page_attendance_id_peoplebar"
            android:layout_marginLeft="5dp"
            android:src="@drawable/store_33_image_textballoon" />
        <!-- money -->

        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/home_my_page_attendance_id_textballoon"
            android:layout_alignLeft="@id/home_my_page_attendance_id_textballoon"
            android:layout_alignRight="@id/home_my_page_attendance_id_textballoon"
            android:layout_marginBottom="21dp"
            android:layout_marginRight="5dp"
            android:gravity="right"
            android:textColor="#000000" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@id/home_my_page_attendance_id_peoplebar"

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/common_bg_pinkpattern" >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal = "true"
        android:layout_marginTop="12.5dp"
        android:src="@drawable/common_bgimg_notice" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.5dp"
        android:layout_marginTop="10.5dp"
        android:onClick="onClickBack"
        android:background="@drawable/rg_drawable_btn_back"/>
    
    
    
    
    <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="58dp"
        android:src="@drawable/common_image_pinkbox_big"/>
    
    <!-- list view -->
    <LinearLayout
        android:id="@+id/homemorehelp_id_listview"
        android:layout_width="335dp"
        android:layout_height="470dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="130dp"
        android:paddingTop="7dp"
        android:orientation="vertical"
         >
        
       

        <ExpandableListView
            android:id="@+id/homemorenotice_id_listview_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:groupIndicator="@drawable/homemorehelp_drawable_expandablelistview_navi"
            
            />
           <!-- android:childIndicator="@drawable/icon"-->
            
    </LinearLayout>
    
    
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="282dp"
        android:layout_marginTop="560dp"
        android:src="@drawable/store_55_image_noticeman" />
    
    
    <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="117.5dp"
        android:src="@drawable/store_55_image_whitebar"/>
    
     <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="75dp"
        android:src="@drawable/store_55_image_mainnotice"/>
    

</RelativeLayout>

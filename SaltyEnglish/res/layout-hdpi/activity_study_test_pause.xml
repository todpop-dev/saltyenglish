<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/test_19_bg_fisherman">

     <RelativeLayout
        android:layout_width="320dp"
        android:layout_height="533dp"
        android:layout_centerInParent="true" >



    
     <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11.66dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/common_bgimg_test"
        />
    

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="216.53dp"
        android:layout_centerHorizontal="true"
        android:onClick="continueTest"
        android:background="@drawable/lvtestpuse_btn_continue"
        />
    
     <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="266.5dp"
        android:layout_centerHorizontal="true"
        android:onClick="testfinish"
        android:background="@drawable/lvtestpuse_btn_testfinish"
        />





     </RelativeLayout>

</RelativeLayout>

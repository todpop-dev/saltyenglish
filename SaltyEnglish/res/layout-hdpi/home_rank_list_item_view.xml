<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <ImageView
        android:id="@+id/home_list_id_rank_crown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0.83dp" />

    <TextView
        android:id="@+id/home_list_id_rank_rank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="8.89dp"
        android:paddingBottom="10dp"
        android:paddingTop="10dp"
        android:textSize="19sp" />

    <ImageView
        android:id="@+id/home_list_id_rank_user_image"
        android:layout_width="44.44dp"
        android:layout_height="41.64dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="44.44dp" />

    <TextView
        android:id="@+id/home_list_id_rank_name_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="115.56dp"
        android:textColor="#000000"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/home_list_rank_id_user_fraction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="8.89dp"
        android:textColor="#000000"
        android:textSize="17sp" />

</RelativeLayout>
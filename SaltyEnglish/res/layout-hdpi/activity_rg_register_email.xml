<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/common_bg_darkyellowpattern"
    android:id="@+id/rgregisteremail_id_main_activity"
   >

     <RelativeLayout
        android:layout_width="320dp"
        android:layout_height="533dp"
        android:layout_centerInParent="true" >



   
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="9.33dp"
        android:layout_marginTop="8.74dp"
        android:onClick="onClickBack"
        android:background="@drawable/rg_drawable_btn_back"/>
    
   <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    	android:layout_marginLeft="78.67dp"
    	android:layout_marginTop="89.94dp"
    	android:src="@drawable/register_4_bgimg_email"
        />
   
   <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    	android:layout_marginLeft="78.67dp"
    	android:layout_marginTop="259.84dp"
    	android:src="@drawable/register_21_bgimg_nickname"
        />
   <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    	android:layout_marginLeft="78.67dp"
    	android:layout_marginTop="327.3dp"
    	android:src="@drawable/register_21_bgimg_recommendnickname"
        />
   
   
   
   
   

    <EditText 
        android:id="@+id/rgregisteremail_id_email"
        android:layout_width="252.44dp"
        android:layout_height="52.47dp"
        android:layout_marginLeft="48.89dp"
        android:layout_marginTop="109.93dp"
       	android:singleLine="true"
       	android:paddingLeft="25dp"
       	android:paddingRight="20dp"
       	android:inputType="textEmailAddress"
       	android:hint="salty-english@todpop.co.kr"
       	android:textSize="15sp"
        android:background="@drawable/common_textbox_long"
        />
    
    <EditText 
        android:id="@+id/rgregisteremail_id_emailpwd"
        android:layout_width="252.44dp"
        android:layout_height="52.47dp"
        android:layout_marginLeft="48.89dp"
        android:layout_marginTop="157.82dp"
        android:singleLine="true"
       	android:paddingLeft="25dp"
       	android:paddingRight="20dp"
       	android:inputType="textPassword"
        android:hint="비밀번호 입력(영문,숫자혼합8-12자)"
        android:textSize="15sp"
        android:background="@drawable/common_textbox_long"
        />
    
    <EditText 
        android:id="@+id/rgregisteremail_id_emailpwdcheck"
        android:layout_width="252.44dp"
        android:layout_height="52.47dp"
        android:layout_marginLeft="48.89dp"
        android:layout_marginTop="206.12dp"
        android:singleLine="true"
       	android:paddingLeft="25dp"
       	android:paddingRight="20dp"
       	android:inputType="textPassword"
        android:hint="비밀번호 확인"
        android:textSize="15sp"
        android:background="@drawable/common_textbox_long"
        />
    
    <EditText 
        android:id="@+id/rgregisteremail_id_nickname"
        android:layout_width="169.78dp"
        android:layout_height="52.47dp"
        android:layout_marginLeft="48.89dp"
        android:layout_marginTop="275.24dp"
        android:singleLine="true"
        android:paddingLeft="25dp"
       	android:paddingRight="20dp"
       	android:inputType="textEmailAddress"
        android:hint="예) 짭짤"
        android:textSize="15sp"
        android:background="@drawable/common_textbox_short"
        />
    
    <EditText 
        android:id="@+id/rgregisteremail_id_nicknamerefre"
        android:layout_width="220.44dp"
        android:layout_height="52.47dp"
        android:layout_marginLeft="48.89dp"
        android:layout_marginTop="340.2dp"
        android:singleLine="true"
        android:paddingLeft="25dp"
       	android:paddingRight="20dp"
        android:hint="추천인이 없는 경우 빈칸으로 남겨주세요"
        android:textSize="15sp"
        android:background="@drawable/common_textbox_short"
        />
    
    <Button 
        android:id="@+id/rgregisteremail_id_checknickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="217.78dp"
        android:layout_marginTop="284.82dp"
        android:background="@drawable/rgregisteremail_drawable_btn_checknickname"
        android:onClick="checkDuplicatedNickname"
        />
    
    <Button 
        android:id="@+id/rgregisteremail_id_donebtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="409.33dp"
        android:background="@drawable/rgregisteremail_drawable_btn_next_activity"
        android:onClick="bridgeToEmailInfoActivity"/>
    
    <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="31.56dp"
        android:layout_marginTop="79.53dp"
        android:src="@drawable/register_19_image_emailman"/>
    
    <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="42.67dp"
        android:layout_marginTop="258.59dp"
        android:src="@drawable/register_21_image_nicknamegirl"/>
    
    <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="41.33dp"
        android:layout_marginTop="324.38dp"
        android:src="@drawable/register_21_image_recommendman"/>
    
     <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    	android:layout_marginLeft="62.67dp"
    	android:layout_marginTop="470.96dp"
    	android:src="@drawable/common_image_bgcharacter4"
        />
     
     <ImageView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
    	android:layout_centerHorizontal="true"
    	android:layout_marginTop="9.99dp"
    	android:src="@drawable/common_bgimg_register"
        />





     </RelativeLayout>

</RelativeLayout>
